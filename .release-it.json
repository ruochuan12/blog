{
	"github": {
	  "release": false,
	  "tokenRef": "GITHUB_TOKEN"
	},
	"git": {
	  "commitMessage": "release: v${version}"
	},
	"npm": {
	  "publish": false
	},
	"hooks": {
	  "after:bump": "echo 更新版本成功"
	},
	"plugins": {
	  "@release-it/conventional-changelog": {
		"preset": "angular",
		"infile": "CHANGELOG.md"
	  }
	}
  }
